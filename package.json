{
  "name": "vite-vue-ts-antd-starter",
  "type": "module",
  "version": "0.1.0",
  "author": "zhixiangyao <blueskyzx@icloud.com> <https://github.com/zhixiangyao>",
  "license": "MIT",
  "homepage": "https://github.com/zhixiangyao/vite-vue-ts-antd-starter",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhixiangyao/vite-vue-ts-antd-starter.git"
  },
  "engines": {
    "node": ">=22",
    "pnpm": ">=9"
  },
  "scripts": {
    "dev": "vite --host",
    "build": "vue-tsc -b && vite build",
    "preview": "npm run build && vite preview",
    "preview:dist": "vite preview",
    "lint": "eslint --cache --cache-location node_modules/.cache/eslint/",
    "lint:stylelint": "stylelint \"./**/*.{vue,css}\" --cache --cache-location node_modules/.cache/stylelint/",
    "lint-staged": "lint-staged",
    "postinstall": "simple-git-hooks"
  },
  "dependencies": {
    "@ant-design/icons-vue": "7.0.1",
    "@vueuse/core": "13.1.0",
    "ant-design-vue": "4.2.6",
    "pinia": "3.0.2",
    "vue": "3.5.13",
    "vue-draggable-plus": "0.6.0",
    "vue-router": "4.5.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "4.12.0",
    "@types/node": "22.14.1",
    "@types/nprogress": "0.2.3",
    "@typescript-eslint/eslint-plugin": "8.31.0",
    "@unocss/preset-rem-to-px": "66.1.0-beta.12",
    "@unocss/reset": "66.1.0-beta.12",
    "@vitejs/plugin-vue": "5.2.3",
    "@vitejs/plugin-vue-jsx": "4.1.2",
    "@vue/compiler-sfc": "3.5.13",
    "@vue/tsconfig": "0.7.0",
    "autoprefixer": "10.4.21",
    "eslint": "9.25.1",
    "lint-staged": "15.5.1",
    "simple-git-hooks": "2.13.0",
    "stylelint": "16.19.0",
    "stylelint-config-recommended-vue": "1.6.0",
    "stylelint-config-standard": "38.0.0",
    "stylelint-order": "7.0.0",
    "typescript": "5.8.3",
    "unocss": "66.1.0-beta.12",
    "unplugin-auto-import": "19.1.2",
    "unplugin-vue-components": "^28.5.0",
    "vite": "6.3.3",
    "vue-tsc": "2.2.10"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": [
      "eslint --cache --cache-location node_modules/.cache/eslint/"
    ],
    "./**/*.{vue,css}": [
      "stylelint --cache --cache-location node_modules/.cache/stylelint/"
    ]
  }
}
